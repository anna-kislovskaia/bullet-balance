{
  "name": "@bullet-balance/components",
  "version": "1.0.0",
  "description": "Portfolio Modeling Tool UI",
  "main": "dist/index.js",
  "repository": " https://github.com/anna-kislovskaia/bullet-balance.git",
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "recharts": "1.8.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rxjs": "6.3.3"
  },
  "devDependencies": {
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.7",
    "@types/recharts": "1.8.10",
    "@types/webpack-env": "1.15.2",
    "typescript": "3.8.3",
    "onchange": "7.1.0",
    "concurrently": "4.1.0",
    "copyfiles": "2.4.1"
  },
  "scripts": {
    "compile": "tsc",
    "ts:watch": "tsc -w",
    "copy:styles": "copyfiles -u 1 src/**/*.scss dist",
    "styles:watch": "onchange --await-write-finish 2000 \"src/**/*.scss\" -- npm run copy:styles",
    "portfolio:start": "concurrently npm:ts:watch npm:styles:watch"    
  }
}
